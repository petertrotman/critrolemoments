  "bugs": {
    "url": "https://github.com/petertrotman/critrolemoments/issues"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      "stage-2"
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "homepage": "https://github.com/petertrotman/critrolemoments#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
